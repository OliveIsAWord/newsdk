TARGET "XrLink"
	#IF ( STRCMP PLATFORM "fox32os" )
		FILE "<bld>/xrlink.fxf"
	#ELSE
		FILE "<bld>/xrlink.exe"
	#END
	
	DEPEND_LIBRARY ( 0 )

	INCDIR "./include"
	SOURCES "./Linker"

	#IF ( STRCMP ARCHITECTURE "CHost" )
		SOURCES "./Linker/CHost"
	#END

	#IF ( STRCMP PLATFORM "fox32os" )
		SOURCES "./Linker/fox32os"

		LINKOPT "-fxf"
	#END
END