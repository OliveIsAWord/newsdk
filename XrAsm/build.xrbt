TARGET "XrAsm"
	#IF ( STRCMP PLATFORM "fox32os" )
		FILE "<bld>/xrasm.fxf"
	#ELSE
		FILE "<bld>/xrasm.exe"
	#END

	DEPEND_LIBRARY ( 0 )

	INCDIR "./include"
	SOURCES "./Frontend"

	SOURCES "./xr17032"
	SOURCES "./fox32"

	#IF ( STRCMP ARCHITECTURE "CHost" )
		SOURCES "./Frontend/CHost"
	#END

	#IF ( STRCMP PLATFORM "fox32os" )
		SOURCES "./Frontend/fox32os"

		LINKOPT "-fxf"
	#END
END